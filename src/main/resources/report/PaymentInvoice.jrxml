	<parameter name="P_paymentId" class="java.lang.String"/>
	<query language="SQL"><![CDATA[SELECT 
    p.paymentId, 
    pt.name, 
    ts.sessionId, 
    ts.sessionDate, 
    p.amount, 
    p.paymentDate, 
    p.status
FROM 
    Payment p 
JOIN 
    Patient pt ON p.patientId = pt.patientId 
JOIN 
    Therapy_Session ts ON ts.patientId = pt.patientId 
WHERE 
    p.paymentId =$P{P_paymentId}
]]></query>
